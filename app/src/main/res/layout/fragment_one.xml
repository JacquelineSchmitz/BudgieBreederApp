<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <Button
            android:id="@+id/btn_nest_fragment_one"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:backgroundTint="#F4B9B8"
            android:bottomLeftRadius="30dp"
            android:bottomRightRadius="30dp"
            android:text="Nester"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="normal"
            android:topLeftRadius="30dp"
            android:topRightRadius="30dp"
            app:cornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="@+id/imageViewbbschrift"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageViewbbschrift"
            app:layout_constraintTop_toBottomOf="@+id/imageViewwellifoto" />

        <Button
            android:id="@+id/btn_fotos_fragment_one"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:text="Fotos"
            android:textColor="@color/black"
            android:backgroundTint="#85D2D0"
            android:textSize="16sp"
            app:cornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="@+id/btn_nest_fragment_one"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/btn_nest_fragment_one"
            app:layout_constraintTop_toBottomOf="@+id/btn_nest_fragment_one" />

        <Button
            android:id="@+id/btn_flirtbox_fragment_one"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:text="FlirtBox"
            android:textColor="@color/black"
            android:backgroundTint="#887BB0"
            android:textSize="16sp"
            app:cornerRadius="10dp"
            app:layout_constraintEnd_toEndOf="@+id/btn_fotos_fragment_one"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/btn_fotos_fragment_one"
            app:layout_constraintTop_toBottomOf="@+id/btn_fotos_fragment_one" />

        <ImageView
            android:id="@+id/background_fragment_one"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/backgroundfeder" />

        <ImageView
            android:id="@+id/imageViewwellifoto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="-150dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/wellihomescreeen" />

        <ImageView
            android:id="@+id/imageViewbbschrift"
            android:layout_width="279dp"
            android:layout_height="130dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.39"
            app:srcCompat="@drawable/textbudgieb" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>